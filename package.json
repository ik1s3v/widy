{
	"name": "widy",
	"private": true,
	"version": "0.2.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"lint": "npx @biomejs/biome lint --write",
		"format": "npx @biomejs/biome format --write",
		"build": "tsc && vite build",
		"build:production": "npm run auc-fighter-adapter:build && npm run widget:build && tauri build",
		"widget:dev": "cross-env NODE_ENV=development vite build -c vite.config.widget.ts --watch",
		"auc-fighter-adapter:dev": "cross-env NODE_ENV=development vite build -c vite.config.auc_fighter_adapter.ts --watch",
		"widget:build": "cross-env NODE_ENV=production vite build -c vite.config.widget.ts",
		"auc-fighter-adapter:build": "cross-env NODE_ENV=production vite build -c vite.config.auc_fighter_adapter.ts",
		"preview": "vite preview",
		"tauri": "tauri"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@mui/icons-material": "^6.4.7",
		"@mui/material": "^6.4.7",
		"@mui/x-date-pickers": "^8.11.1",
		"@reduxjs/toolkit": "^2.6.1",
		"@tauri-apps/api": "^2",
		"@tauri-apps/plugin-dialog": "^2.2.0",
		"@tauri-apps/plugin-fs": "^2.2.0",
		"@tauri-apps/plugin-global-shortcut": "^2.2.0",
		"@tauri-apps/plugin-log": "^2.3.1",
		"@tauri-apps/plugin-opener": "^2.5.0",
		"@tauri-apps/plugin-process": "^2.2.1",
		"@tauri-apps/plugin-updater": "^2.6.1",
		"@tauri-apps/plugin-websocket": "^2.4.0",
		"@vis.gl/react-google-maps": "^1.5.4",
		"cross-env": "^7.0.3",
		"dayjs": "^1.11.13",
		"react": "^18.3.1",
		"react-color": "^2.19.3",
		"react-custom-roulette": "^1.4.1",
		"react-dom": "^18.3.1",
		"react-i18next": "^15.4.1",
		"react-infinite-scroller": "^1.2.6",
		"react-number-format": "^5.4.3",
		"react-phone-input-2": "^2.15.1",
		"react-redux": "^9.2.0",
		"react-router": "^7.2.0",
