		"@types/react-color": "^3.0.13",
		"@types/react-dom": "^18.3.1",
		"@types/react-infinite-scroller": "^1.2.5",
		"@types/react-virtualized": "^9.22.2",
		"@types/string-similarity": "^4.0.2",
		"@vitejs/plugin-react": "^5.0.0",
		"typescript": "~5.6.2",
		"vite": "^7.1.2"
